doctype html
html
  head
    include includes/headContent.pug
  body(id="profiles")
    include includes/nav.pug
    div(class="centre")

      h1 Meet the team
      table(id="profiles")
        thead
          tr
            th Name
            th Age
            th Gender
            th Comment
        each val in JSON.parse(profiles)
          tr
            td= val.name
            td= val.age
            td= val.gender
            td= val.comments
